    <id name="Login" />
    <property name="Senha" />
		<property name="Roles" />

    <property name="Nome" />
    <property name="Email" />
    <property name="Cpf" />
    <property name="DataNascimento" />

    <property name="Ddd" />
    <property name="NumeroTelefone" />

    <property name="Cep" />
